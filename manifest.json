{
    "manifest_version": 2,

    "name": "Toggle PSD",
    "description": "Toggle a PSD overlay of the website you are working on.",

    "version": "1.0",

    "permission": [
        "https://secure.flickr.com/"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}